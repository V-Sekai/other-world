{
  "version": "0.2.0",
  "options": { 
    "env": {
      "RUST_LOG": "log",
      "MVSQLITE_DATA_PLANE": "http://192.168.0.39:7000"
    }
  },
  "configurations": [
    {
      "name": "Run the editor with V-Sekai game",
      "type": "lldb",
