config_version=5

[application]

config/name="Crappy Contraptions"
config/version="1.3.0"
run/main_scene="res://crappy_contraptions/ui/boot_splash.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="crappy-contraptions"
config/features=PackedStringArray("4.3", "Double Precision", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
config/icon="res://icon.svg"

[autoload]

SnailInput="*res://addons/snail_input/snail_input.gd"
SnailTransition="*res://addons/snail_transition/snail_transition.gd"
DD="*res://addons/zylann.debug_draw/debug_draw.gd"
Globals="*res://crappy_contraptions/scenes/globals.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=800
window/size/extend_to_title=true
window/energy_saving/keep_screen_on=false

[editor_plugins]

enabled=PackedStringArray("res://addons/object_inspector/plugin.cfg", "res://addons/smoothing/plugin.cfg", "res://addons/snail_input/plugin.cfg", "res://addons/snail_transition/plugin.cfg")

[filesystem]

import/blender/enabled=false

